      "environment": [
        {
          "name": "DX12_DEBUG_LAYER",
          "value": "0"
        },
        {
          "name": "DX12_GPU_VALIDATION",
          "value": "0"
        },
        {
          "name": "_NT_SYMBOL_PATH",
          "value": ""
        },
        {
          "name": "_NT_ALT_SYMBOL_PATH",
          "value": ""
        }
      ],
      "console": "internalConsole",
      "symbolSearchPath": "",
      "justMyCode": true,
      "logging": {
        "moduleLoad": false,
        "programOutput": true,
        "trace": false,
        "engineLogging": false
